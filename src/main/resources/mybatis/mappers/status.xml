<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="status">
    <select id="select" parameterType="statusvo" resultType="statusvo">
    <![CDATA[
        select no, name
        from status
        where no=#{no}
    ]]>  
    </select>
    
    <select id="selectList" resultType="statusvo">
    <![CDATA[
        select no, name
        from status
    ]]>  
    </select>
</mapper>
